{
  "name": "yamabiko",
  "version": "1.0.0",
  "description": "Eleventy blog",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "start": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --watch --incremental",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "stop": "kill -9 $(lsof -t -i :8080)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soundwanders/yamabiko.git"
  },
  "author": {
    "name": "soundwanders",
    "email": "soundwanders@users.noreply.github.com",
    "url": "https://yamabiko.netlify.app"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soundwanders/yamabiko/issues"
  },
  "homepage": "https://yamabiko.netlify.app",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-beta.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "clean-css": "^5.3.2",
    "luxon": "^3.3.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "prism-themes": "^1.9.0",
    "simpledotcss": "2.1.1",
    "simplelightbox": "^2.14.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "smashtest": "^1.8.4"
  }
}
